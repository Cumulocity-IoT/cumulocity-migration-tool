{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es6",
    "module": "es2020",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "angularCompilerOptions": {
    "enableIvy": false
  }
}
